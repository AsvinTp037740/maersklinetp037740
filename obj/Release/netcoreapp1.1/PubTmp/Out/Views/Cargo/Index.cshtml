@model IEnumerable<MaerskLine.Models.Cargocargo>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.CargoWeight)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CargoMaterial)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.WarehouseNo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CargoTag)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CargoWeight)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CargoMaterial)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WarehouseNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CargoTag)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
